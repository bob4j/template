<?xml version="1.0" encoding="UTF-8"?>
<Configuration>

	<Appenders>

		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss,SSS} %m%n" />
		</Console>

	</Appenders>
	
	<Loggers>
		
		<Logger name="com.openu" level="debug" />
		
		<Root level="WARN">
			<AppenderRef ref="Console" />
		</Root>

	</Loggers>

</Configuration>